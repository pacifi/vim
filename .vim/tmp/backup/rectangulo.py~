#! /usr/bin/python
# coding: utf-8

class Rectangulo(object):
    def __init__(self, x, y):
        self.ancho = x
        self.alto = y

    def perimetro(self):
        return 2*self.ancho + 2*self.alto

    def area(self):
        return self.ancho*self.alto

class Cuadrado(Rectangulo):
    def __init__(self, x):
        self.ancho = x
        self.alto = x
