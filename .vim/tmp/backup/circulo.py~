#! /usr/bin/python
# coding: utf-8

class Circulo(object):
    pi = 3.141592

    def __init__(self, radius =1):
        self.raius = radius
    def area(self):
        return self.radius * self.radius * Circulo.pi

    def setRadius(self, radius):
        self.radius = radius

    def getRadius(self):
        return self.radius
